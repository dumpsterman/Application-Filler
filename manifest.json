{
  "manifest_version": 2,

  "name": "Application Filler Plugin",
  "description": "This extension will fill an application for you",
  "version": "0.01",
  "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'",
  "background": {
    "scripts": ["Context_Menu.js","jquery-3.3.1.js"],
    "persistent": true
  },
  "browser_action": {
   "default_icon": "icon.png",
   "default_popup": "ui.html"
  },
  "permissions": [
   "activeTab",
   "storage",
   "contextMenus"
   ]
}
